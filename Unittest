import unittest

class UnitTestFramkwork(unittest.TestCase):
    
    def test_uppercase(self):
        test_string = 'comp'
        result = test_string.upper()
        self.assertEqual(result,'COMP')

    def test_all_uppercase(self):
        self.assertTrue('SCI'.isupper())
        self.assertFalse('Sci'.isupper())

if __name__ == '__main__':
    unittest.main()
